{
  "name": "nextrush-performance-benchmarks",
  "version": "1.0.0",
  "description": "Professional performance benchmarks for NextRush v2 vs Express, Koa, and Fastify",
  "private": true,
  "scripts": {
    "setup": "pnpm install",
    "bench": "./scripts/run-all.sh",
    "bench:quick": "./scripts/quick-benchmark.sh",
    "bench:autocannon": "./tests/autocannon/run-all.sh",
    "bench:k6": "./tests/k6/run-all.sh",
    "analyze": "node scripts/analyze.js",
    "report": "node scripts/generate-report.js",
    "compare": "node scripts/analyze.js && node scripts/generate-report.js && cat results/BENCHMARK_RESULTS.md",
    "clean": "rm -rf results/*.json results/*.txt results/*.md"
  },
  "devDependencies": {
    "autocannon": "^8.0.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "express": "^5.1.0",
    "fastify": "^5.6.1",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^14.0.0",
    "ora": "^9.0.0",
    "pidusage": "^4.0.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
