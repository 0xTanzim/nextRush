{
  "name": "nextrush",
  "version": "1.1.1",
  "description": "A modern, fast HTTP server framework for Node.js with Express-compatible API, TypeScript-first design, and clean architecture. Zero dependencies, maximum performance, familiar syntax.",
  "keywords": [
    "typescript",
    "http",
    "server",
    "framework",
    "express",
    "lightweight",
    "backend",
    "routing",
    "api",
    "fast",
    "modern",
    "zero-dependencies",
    "web-framework",
    "nodejs",
    "rest-api",
    "clean-architecture",
    "dependency-injection",
    "testable",
    "middleware",
    "performance"
  ],
  "homepage": "https://github.com/0xTanzim/nextrush#readme",
  "bugs": {
    "url": "https://github.com/0xTanzim/nextrush/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xTanzim/nextrush.git"
  },
  "license": "MIT",
  "author": {
    "name": "Tanzim Hossain",
    "email": "tanzimhossain2@gmail.com",
    "url": "https://github.com/0xTanzim"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "examples/*.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only examples/express-style.ts",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:express": "node examples/simple-express.js",
    "example:typescript": "ts-node examples/typescript-example.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && npm run validate",
    "validate": "node scripts/validate-package.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo \"Add linting here\"",
    "format": "echo \"Add formatting here\""
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
